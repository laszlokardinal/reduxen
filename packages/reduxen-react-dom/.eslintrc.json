{
  "parser": "babel-eslint",
  "extends": [
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "chai-friendly",
    "mocha"
  ],
  "rules": {
    "no-undefined": 0,
    "no-unused-expressions": 0,
    "chai-friendly/no-unused-expressions": 2,
    "mocha/no-exclusive-tests": "error"
  }
}
